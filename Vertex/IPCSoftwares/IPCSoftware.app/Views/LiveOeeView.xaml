<UserControl x:Class="IPCSoftware.App.Views.LiveOeeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:IPCSoftware.App.ViewModels">

    <Grid>
        <TextBlock Text="{Binding RawMessage}"
                   FontSize="14"
                   Foreground="Black"
                   Margin="10"/>
        <TextBlock Text="This is Live View"
           FontSize="14"
           Foreground="black"
           Margin="10"/>
        <TextBlock Text="{Binding OperatingMinutes}" Foreground="Black" Margin="30" FontSize="24"/>


    </Grid>

</UserControl>
